{
  "externalRoutes": {},
  "routerToken": "",
  "disableRouterEndpoint": false,
  "debugLogging": true,
  "queuerDB": 0,
  "requestTimeout": 30,
  "rateLimiter": {
    "db": 14,
    "limit": -1,
    "duration": 60
  },
  "forceMessageSignature": false,
  "signatureSharedSecret": "d632dd6d-fb75-44cc-bdbf-ee1364f3716c",
  "cors": {
    "access-control-allow-origin": "*",
    "access-control-allow-methods": "GET, POST, PUT, DELETE, OPTIONS",
    "access-control-allow-headers": "accept, authorization, cache-control, content-type, x-requested-with",
    "access-control-allow-credentials": "true",
    "access-control-Max-Age": 10
  },
  "hydra": {
    "serviceName": "hydra-router",
    "serviceDescription": "Service Router",
    "serviceIP": "",
    "servicePort": "80",
    "serviceType": "router",
    "plugins": {
      "hydraLogger": {
        "logToConsole": true,
        "onlyLogLocally": false
      },
      "dontuse-logger": {
        "logRequests": true,
        "noFile": true,
        "toConsole": true,
        "redact": ["password"],
        "elasticsearch": {
          "rotate": "daily",
          "host": "host",
          "port": 9200,
          "index": "hydra"
        }
      },
      "dontuse-loggly": {
        "method": "POST",
        "protocol": "http",
        "hostname": "logs-01.loggly.com",
        "port": 80,
        "path": "/inputs/{token-here}/tag/http/",
        "logToConsole": true,
        "onlyLogLocally": false
      }
    },
    "redis": {
      "url": "redis://redis:6379/15"
    }
  }
}
